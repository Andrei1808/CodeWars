function isDivisible(n, x, y) {
  if(n % y == 0 && n % x == 0){
return true};
  return false;
}



function remainder(a, b){
  // Divide the larger argument by the smaller argument and return the remainder
  if(a > b){
return a % b;
}
  else{
 return b % a;}
}



function litres(time) {
  if(time >= 0){
    x = time * 0.5
};
  return Math.floor(x);
}
